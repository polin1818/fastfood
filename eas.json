{
  "expo": {
    "name": "RecetteAfrique",
    "slug": "RecetteAfrique",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "recetteafrique",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.recetteafrique.app",
      "icon": "./assets/logo.png",
      "infoPlist": {
        "UILaunchImages": [
          {
            "UILaunchImageMinimumOSVersion": "8.0",
            "UILaunchImageName": "logo",
            "UILaunchImageOrientation": "Portrait",
            "UILaunchImageSize": "{375, 667}"
          }
        ]
      }
    },
    "android": {
      "package": "com.recetteafrique.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/logo.png",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-asset",
      "expo-font",
      "expo-web-browser",
      "expo-localization",
      "react-native-video"
    ],
    "extra": {
      "eas": {
        "projectId": "abe6f047-4b5d-40a5-8ef8-5bd6b17469c9"
      }
    }
  }
}
